
<div class="body-contents">
    <div class="topNav">
        <a class ="navButton" href="/projectboards">Manage Projects</a>
    </div>
    <div class="projectBoard">
        <div class="header">{{projectBoard.name}}</div>
        <div class= "projectContents">
            {{#each lists}}
                <div class ="listCard">
                    <div class="header">{{this.name}}</div>
                    <div class="taskContainer" id="container">
                        {{#each this.tasks}}
                            <div class="taskCard">
                                <div class="taskHeader">{{this.description}}</div>
                            </div>
                        {{/each}}
                        <div id="addTask" class="taskCard">
                            <script type="text/javascript">
                                addTask('{{this.name}}', 'To Do', {{this.id}});
                            </script>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
</div>



